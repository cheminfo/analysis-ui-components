{
  "name": "analysis-ui-components",
  "version": "0.18.0",
  "description": "React components to build analysis UI",
  "exports": {
    "./app": {
      "types": "./lib-esm/app/index.d.ts",
      "require": "./lib/app/index.js",
      "default": "./lib-esm/app/index.js"
    },
    "./app-data": {
      "types": "./lib-esm/app-data/index.d.ts",
      "require": "./lib/app-data/index.js",
      "default": "./lib-esm/app-data/index.js"
    },
    "./ui": {
      "types": "./lib-esm/components/index.d.ts",
      "require": "./lib/components/index.js",
      "default": "./lib-esm/components/index.js"
    }
  },
  "files": [
    "lib",
    "lib-esm",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-ts && npm run fix-react-shadow",
    "build-all": "npm run build-app && npm run build-stories && mv build dist/stories",
    "build-app": "cross-env NO_MINIFY=true vite build",
    "build-stories": "ladle build",
    "build-ts": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "createData": "npm run prepack && node scripts/createData",
    "dev": "ladle serve",
    "dev-app": "vite --open",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint -- --fix",
    "fix-react-shadow": "node scripts/fix-react-shadow.mjs",
    "playwright": "npm run playwright-ct -- --project chromium && npm run playwright-e2e -- --project chromium",
    "playwright-ct": "playwright test -c playwright-ct.config.ts",
    "playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "prepack": "npm run build",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "test": "npm run playwright && npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest run --coverage "
  },
  "volta": {
    "node": "16.18.0"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@headlessui/react": "^1.7.4",
    "@lukeed/uuid": "^2.0.0",
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-table": "^8.5.22",
    "biologic-converter": "^0.4.0",
    "cheminfo-types": "^1.4.0",
    "filelist-utils": "^1.2.0",
    "immer": "^9.0.16",
    "jcampconverter": "^9.0.2",
    "lodash": "^4.17.21",
    "ml-gsd": "^12.1.2",
    "ml-peak-shape-generator": "^4.1.2",
    "netcdfjs": "^2.0.2",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^3.1.4",
    "react-popper": "^2.3.0",
    "react-shadow": "^19.0.3",
    "spc-parser": "^0.7.1",
    "tinycolor2": "^1.4.2",
    "use-resize-observer": "^9.0.2",
    "wdf-parser": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@ladle/react": "^2.4.5",
    "@playwright/experimental-ct-react": "^1.27.1",
    "@playwright/test": "^1.27.1",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-inspector": "^4.0.2",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "cheminfo-font": "^1.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-zakodium": "^7.0.0",
    "isotopic-distribution": "^1.4.15",
    "ml-signal-processing": "^0.5.2",
    "ml-spectra-processing": "^11.13.0",
    "ms-spectrum": "^1.6.15",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-d3-utils": "^0.6.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-inspector": "^6.0.1",
    "react-kbs": "^2.1.0",
    "react-ocl": "^5.0.0",
    "react-plot": "^0.20.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.24.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium-oss/analysis-ui-components.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium-oss/analysis-ui-components/issues"
  },
  "homepage": "https://github.com/zakodium-oss/analysis-ui-components#readme"
}
