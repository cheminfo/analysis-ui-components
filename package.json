{
  "name": "analysis-ui-components",
  "version": "0.12.0",
  "description": "React components to build analysis UI",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-esm/index.d.ts",
  "files": [
    "lib",
    "lib-esm",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-ts",
    "build-storybook": "build-storybook",
    "build-ts": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "dev": "npm run storybook",
    "eslint": "eslint src stories --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run build",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "storybook": "start-storybook -s ./public -p 6006",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "playwright test",
    "vite": "vite --open",
    "vite-build": "cross-env NO_MINIFY=true vite build"
  },
  "volta": {
    "node": "16.15.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "immer": "^9.0.12",
    "react-dropzone": "^12.1.0",
    "react-shadow": "^19.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@playwright/test": "^1.21.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-storysource": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@vitejs/plugin-react": "^1.3.1",
    "@zakodium/eslint-config": "^5.1.1",
    "babel-loader": "^8.2.5",
    "cheminfo-font": "^1.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-ocl": "^4.3.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium-oss/analysis-ui-components.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium-oss/analysis-ui-components/issues"
  },
  "homepage": "https://github.com/zakodium-oss/analysis-ui-components#readme"
}
