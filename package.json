{
  "name": "analysis-ui-components",
  "version": "0.16.0",
  "description": "React components to build analysis UI",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-esm/index.d.ts",
  "files": [
    "lib",
    "lib-esm",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-ts && npm run fix-react-shadow",
    "build-all": "npm run build-app && npm run build-stories && mv build dist/stories",
    "build-app": "cross-env NO_MINIFY=true vite build",
    "build-stories": "ladle build",
    "build-ts": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "createData": "npm run prepack && node scripts/createData",
    "dev": "ladle serve",
    "dev-app": "vite --open",
    "eslint": "eslint src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "fix-react-shadow": "node scripts/fix-react-shadow.mjs",
    "playwright": "npm run playwright-ct -- --project chromium && npm run playwright-e2e -- --project chromium",
    "playwright-ct": "playwright test -c playwright-ct.config.ts",
    "playwright-e2e": "playwright test -c playwright-e2e.config.ts",
    "prepack": "npm run build",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "test": "npm run playwright && npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest run --coverage "
  },
  "volta": {
    "node": "16.17.0"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@lukeed/uuid": "^2.0.0",
    "@tanstack/react-table": "^8.1.4",
    "biologic-converter": "^0.0.1",
    "cheminfo-types": "^1.4.0",
    "filelist-utils": "^0.7.1",
    "immer": "^9.0.15",
    "jcampconverter": "^9.0.2",
    "lodash": "^4.17.21",
    "ml-gsd": "^12.1.0",
    "ml-peak-shape-generator": "^4.1.2",
    "react-dropzone": "^14.2.2",
    "react-error-boundary": "^3.1.4",
    "react-shadow": "^19.0.3",
    "spc-parser": "^0.5.2",
    "tinycolor2": "^1.4.2",
    "use-resize-observer": "^9.0.2",
    "wdf-parser": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.18.9",
    "@ladle/react": "^2.4.2",
    "@playwright/experimental-ct-react": "^1.25.2",
    "@playwright/test": "^1.25.2",
    "@types/lodash": "^4.14.184",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-inspector": "^4.0.2",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.1",
    "@zakodium/eslint-config": "^6.0.0",
    "babel-loader": "^8.2.5",
    "cheminfo-font": "^1.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "ml-signal-processing": "^0.5.2",
    "ml-spectra-processing": "^11.12.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-inspector": "^6.0.1",
    "react-ocl": "^5.0.0",
    "react-plot": "^0.20.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium-oss/analysis-ui-components.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium-oss/analysis-ui-components/issues"
  },
  "homepage": "https://github.com/zakodium-oss/analysis-ui-components#readme"
}
